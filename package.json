{
  "type": "module",
  "name": "@datastructures-js/heap",
  "version": "4.3.4",
  "description": "Min/Max Heap & Heap Sort implementation in javascript",
  "types": "index.d.ts",
  "main": "index.js",
  "scripts": {
    "test": "node ./data-algo/binary-template-simple.js",
    "ag": "clear & node ./data-algo",
    "kr": "clear & node ./kr_fun",
    "leet": "clear & npx nodemon ./index.js",
    "ts": "npx nodemon --watch . --ext ts --exec \"node tsx ./index.ts\"",
    "m1": "node --watch --experimental-strip-types --inspect=9228 ./index.ts",
    "vn": "node --watch --import=tsx ./index.ts",
    "mf": "node --watch --import=tsx ./index.ts",
    "fetch": "set NODE_TLS_REJECT_UNAUTHORIZED=0 && node --watch --import=tsx ./index.ts"
  },
  "dependencies": {
    "@datastructures-js/binary-search-tree": "^5.3.3",
    "@datastructures-js/deque": "^1.0.5",
    "@datastructures-js/heap": "^4.3.3",
    "@datastructures-js/priority-queue": "^6.3.3",
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "tsx": "^4.20.4"
  }
}
